[	{% if detail_list %}		{% for detail in detail_list %}			{				"section_title": "{{ detail.section_title }}",				"data_rows": [					{% for item in detail.data_rows %}						{							"sub_title": "{{ item.sub_title }}",							"content_desc": "{{ item.content_desc }}",							"chart_type": "{{ item.chart_type }}",							"is_folder": "{{ item.is_folder }}",							"next_id": "{{ item.next_id }}"						}{% if not forloop.last %},{% endif %}					{% endfor %}				]			}{% if not forloop.last %},{% endif %}		{% endfor %}	{% endif %}]