[	{% if plot %}	{		"number_of_plots"   : {{ plot.number_of_plots }},		"number_of_records" : {{ plot.number_of_records }},		"plot_identifiers"  : [			{% for item in plot.plot_identifiers %}				"{{ item }}"{% if not forloop.last %},{% endif %}			{% endfor %}		],		"x_labels"          : [			{% for label in plot.x_labels %}				"{{ label }}"{% if not forloop.last %},{% endif %}			{% endfor %}		],		"plot_data_source": {				{% for k, arr in plot.plot_data_source.items %}					"{{ k }}" : [						{% for item in arr %}							{								"x" : {{ item.x }},								"y" : {{ item.y }}							}{% if not forloop.last %},{% endif %}						{% endfor %}					]{% if not forloop.last %},{% endif %}				{% endfor %}		}	}	{% endif %}]